{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "test": "react-scripts test --testMatch=$(pwd)/test/*.test.js",
    "testCI": "CI=true react-scripts test --testMatch=$(pwd)/test/*.test.js",
    "build-css": "node-sass-chokidar src/stylesheets/ -o src/stylesheets/",
    "watch-css": "npm run build-css && node-sass-chokidar src/stylesheets/ -o src/stylesheets/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build-js": "react-scripts build",
    "build": "npm-run-all build-css build-js",
    "pip": "echo hello",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "js-file-download": "^0.4.1",
    "node-sass-chokidar": "^1.0.1",
    "npm-run-all": "^4.1.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-scripts": "1.1.1"
  },
  "devDependencies": {
    "ajv": "^6.2.1",
    "chai": "^4.1.2",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "react-test-renderer": "^16.2.0"
  }
}
